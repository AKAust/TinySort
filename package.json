{
  "name": "tinysort",
  "filename": "tinysort.min.js",
  "version": "3.0.1",
  "description": "TinySort is a small script that sorts HTML elements. It sorts by text- or attribute value, or by that of one of it's children.",
  "main": "dist/tinysort.js",
  "license": "MIT",
  "author": "Ron Valstar <ron@ronvalstar.nl> (http://www.ronvalstar.nl/)",
  "homepage": "http://tinysort.sjeiti.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sjeiti/TinySort.git"
  },
  "scripts": {
    "dist:main": "npm run copy:main && npm run js:main",
    "copy": "npm run copy:dist && copy:img",
    "copy:dist": "copyfiles dist/* distdoc",
    "copy:img": "copyfiles -u 1 doc/style/logo.* doc/style/*.ico distdoc",
    "js:main": "babel src/tinysort.js --out-file dist/tinysort.min.js --presets=es2015,stage-0,minify",
    "_js:doc": "babel doc/js/main.js --out-file distdoc/js/main.js --presets=es2015,stage-0,minify",
    "js:doc": "webpack --config webpack.config.js",
    "js:doc:watch": "chokidar \"doc/**/*.js\" -c \"npm run js:doc\"",
    "lint": "npm run lint:src && npm run lint:doc",
    "lint:src": "eslint src/*.js --fix",
    "lint:doc": "eslint doc/js/*.js --fix",
    "doc": "npm run copy && npm run html && npm run css && npm run js:doc",
    "html": "node task/doc distdoc",
    "html:watch": "chokidar \"doc/**/*.html\" -c \"npm run html\"",
    "css": "npm run css:less && npm run css:prefix && npm run css:minify",
    "css:watch": "chokidar \"doc/**/*.less\" -c \"npm run css\"",
    "css:less": "lessc doc/style/screen.less distdoc/style/screen.css",
    "css:prefix": "postcss distdoc/style/screen.css --use autoprefixer -d distdoc/style/",
    "css:minify": "postcss distdoc/style/screen.css --use cssnano -d distdoc/style/",
    "version": "next-version package.json src/tinysort.js src/jquery.tinysort.js src/tinysort.charorder.js",
    "serve:doc": "node task/serve distdoc 8899",
    "serve:unit": "node task/serve test/unit 8877"
  },
  "scriptss": {
    "build": "node task/build test uglify",
    "jscs": "jscs ./src/",
    "jshint": "jshint ./src/",
    "uglify": "node task/uglify",
    "version": "next-version package.json bower.json src/tinysort.js src/jquery.tinysort.js src/tinysort.charorder.js",
    "test": "node task/test",
    "doc": "node task/jsdoc",
    "_build": "npm run clean && npm run dir && npm run js && npm run copy",
    "_build:watch": "chokidar \"src/js/**/*.js\" -c \"npm run build\"",
    "_dev": "npm run serve | npm run js:watch | npm run copy:watch",
    "_clean": "rimraf dist",
    "_js": "webpack --config webpack.config.js",
    "_js:watch": "chokidar \"src/**/*.js\" -c \"npm run js\"",
    "_copy": "copy src\\index.html dist\\index.html | copy src\\page.html dist\\page.html",
    "_copy:watch": "chokidar \"src/**/*.html\" -c \"npm run copy\"",
    "_dir": "mkdirp dist/js"
  },
  "dependencies": {
    "@emmetio/expand-abbreviation": "^0.7.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-minify": "^0.4.3",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.17.1",
    "bootstrap": "^3.3.7",
    "chokidar-cli": "^1.2.0",
    "copyfiles": "^2.0.0",
    "cssnano": "^4.0.0",
    "eslint": "^3.19.0",
    "express": "^4.16.3",
    "jsdoc-api": "^4.0.3",
    "jsdom": "^8.2.0",
    "less": "^3.5.2",
    "marked": "^0.4.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "next-version": "^0.9.3",
    "node-qunit-phantomjs": "^1.4.0",
    "open": "0.0.5",
    "postcss-cli": "^5.0.1",
    "rimraf": "^2.6.1",
    "serve-static": "^1.13.2",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8"
  },
  "keywords": [
    "sort",
    "element",
    "HTMLElement",
    "Node",
    "list",
    "table",
    "dom",
    "html"
  ],
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/Sjeiti/TinySort.git",
    "basePath": "dist",
    "files": [
      "tinysort.js",
      "tinysort.min.js",
      "tinysort.charorder.js",
      "tinysort.charorder.min.js",
      "jquery.tinysort.js",
      "jquery.tinysort.min.js"
    ]
  }
}
